{
    "name": "jshint",
    "version": "2.10.2",
    "homepage": "http://jshint.com/",
    "description": "Static analysis tool for JavaScript",
    "author": {
        "name": "Anton Kovalyov",
        "email": "anton@kovalyov.net",
        "url": "http://anton.kovalyov.net/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jshint/jshint.git"
    },
    "bugs": {
        "url": "https://github.com/jshint/jshint/issues"
    },
    "bin": {
        "jshint": "./bin/jshint"
    },
    "scripts": {
        "browser-test-server": "node tests/helpers/browser/server",
        "build": "node bin/build",
        "coverage": "istanbul -- cover ./node_modules/nodeunit/bin/nodeunit tests/unit",
        "coverage-report": "npm run coverage && istanbul report html",
        "data": "node scripts/generate-identifier-data",
        "fetch-test262": "git submodule init && git submodule update",
        "pretest": "node ./bin/jshint src && jscs src",
        "changelog": "conventional-changelog -p jshint -i CHANGELOG.md -w",
        "github-release": "conventional-github-releaser -p jshint",
        "test-all": "npm run test-node && npm run test-browser && npm run test-262",
        "test-262": "node tests/test262",
        "test-browser": "node tests/browser",
        "test-cli": "nodeunit tests/cli.js",
        "test-node": "npm run test-unit && npm run test-cli && npm run test-regression",
        "test-regression": "nodeunit tests/regression",
        "test-unit": "nodeunit tests/unit",
        "test": "npm run test-node && npm run test-browser",
        "compile-srcreporterscheckstylejs": "babel src\\reporters\\checkstyle.js -o src\\reporters\\checkstyle.js --require babel-polyfill --presets env",
        "compile-srcreportersjslint_xmljs": "babel src\\reporters\\jslint_xml.js -o src\\reporters\\jslint_xml.js --require babel-polyfill --presets env",
        "compile-testshelperstesthelperjs": "babel tests\\helpers\\testhelper.js -o tests\\helpers\\testhelper.js --require babel-polyfill --presets env",
        "compile-dataes5identifiernamesjs": "babel data\\es5-identifier-names.js -o data\\es5-identifier-names.js --require babel-polyfill --presets es2015-without-strict",
        "compile-dataasciiidentifierdatajs": "babel data\\ascii-identifier-data.js -o data\\ascii-identifier-data.js --require babel-polyfill --presets es2015-without-strict",
        "compile-testshelpersbrowserserverjs": "babel tests\\helpers\\browser\\server.js -o tests\\helpers\\browser\\server.js --require babel-polyfill --presets env",
        "compile-datanonasciiidentifierstartjs": "babel data\\non-ascii-identifier-start.js -o data\\non-ascii-identifier-start.js --require babel-polyfill --presets es2015-without-strict",
        "compile-scriptsremovedevdependenciesjs": "babel scripts\\remove-dev-dependencies.js -o scripts\\remove-dev-dependencies.js --require babel-polyfill --presets env",
        "compile-scriptsgenerateidentifierdatajs": "babel scripts\\generate-identifier-data.js -o scripts\\generate-identifier-data.js --require babel-polyfill --presets env",
        "compile-testshelpersbrowserfixturefsjs": "babel tests\\helpers\\browser\\fixture-fs.js -o tests\\helpers\\browser\\fixture-fs.js --require babel-polyfill --presets env",
        "compile-datanonasciiidentifierpartonlyjs": "babel data\\non-ascii-identifier-part-only.js -o data\\non-ascii-identifier-part-only.js --require babel-polyfill --presets es2015-without-strict",
        "compile-srcclijs": "babel src\\cli.js -o src\\cli.js --require babel-polyfill --presets env",
        "compile-srclexjs": "babel src\\lex.js -o src\\lex.js --require babel-polyfill --presets env",
        "compile-srcregjs": "babel src\\reg.js -o src\\reg.js --require babel-polyfill --presets env",
        "compile-srcvarsjs": "babel src\\vars.js -o src\\vars.js --require babel-polyfill --presets env",
        "compile-srcstatejs": "babel src\\state.js -o src\\state.js --require babel-polyfill --presets env",
        "compile-srcstylejs": "babel src\\style.js -o src\\style.js --require babel-polyfill --presets env",
        "compile-testsclijs": "babel tests\\cli.js -o tests\\cli.js --require babel-polyfill --presets env",
        "compile-srcjshintjs": "babel src\\jshint.js -o src\\jshint.js --require babel-polyfill --presets env",
        "compile-srcoptionsjs": "babel src\\options.js -o src\\options.js --require babel-polyfill --presets env",
        "compile-srcmessagesjs": "babel src\\messages.js -o src\\messages.js --require babel-polyfill --presets env",
        "compile-scriptsbuildjs": "babel scripts\\build.js -o scripts\\build.js --require babel-polyfill --presets env",
        "compile-testsbrowserjs": "babel tests\\browser.js -o tests\\browser.js --require babel-polyfill --presets env",
        "compile-srcnamestackjs": "babel src\\name-stack.js -o src\\name-stack.js --require babel-polyfill --presets env",
        "compile-srcprodparamsjs": "babel src\\prod-params.js -o src\\prod-params.js --require babel-polyfill --presets es2015-without-strict",
        "compile-testsunitcorejs": "babel tests\\unit\\core.js -o tests\\unit\\core.js --require babel-polyfill --presets env",
        "compile-testsunitenvsjs": "babel tests\\unit\\envs.js -o tests\\unit\\envs.js --require babel-polyfill --presets env",
        "compile-examplesreporterjs": "babel examples\\reporter.js -o examples\\reporter.js --require babel-polyfill --presets env",
        "compile-srcscopemanagerjs": "babel src\\scope-manager.js -o src\\scope-manager.js --require babel-polyfill --presets env",
        "compile-testsunitparserjs": "babel tests\\unit\\parser.js -o tests\\unit\\parser.js --require babel-polyfill --presets env",
        "compile-srcreportersunixjs": "babel src\\reporters\\unix.js -o src\\reporters\\unix.js --require babel-polyfill --presets env",
        "compile-teststest262testjs": "babel tests\\test262\\test.js -o tests\\test262\\test.js --require babel-polyfill --presets env",
        "compile-testsunitoptionsjs": "babel tests\\unit\\options.js -o tests\\unit\\options.js --require babel-polyfill --presets env",
        "compile-srcplatformsrhinojs": "babel src\\platforms\\rhino.js -o src\\platforms\\rhino.js --require babel-polyfill --presets env",
        "compile-teststest262indexjs": "babel tests\\test262\\index.js -o tests\\test262\\index.js --require babel-polyfill --presets env",
        "compile-teststest262reportjs": "babel tests\\test262\\report.js -o tests\\test262\\report.js --require babel-polyfill --presets env",
        "compile-srcreportersdefaultjs": "babel src\\reporters\\default.js -o src\\reporters\\default.js --require babel-polyfill --presets env",
        "compile-testshelpersfixturejs": "babel tests\\helpers\\fixture.js -o tests\\helpers\\fixture.js --require babel-polyfill --presets env",
        "compile-testsunitmoduleapijs": "babel tests\\unit\\module-api.js -o tests\\unit\\module-api.js --require babel-polyfill --presets env",
        "compile-srcreportersnon_errorjs": "babel src\\reporters\\non_error.js -o src\\reporters\\non_error.js --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-srcreporterscheckstylejs && npm run compile-srcreportersjslint_xmljs && npm run compile-testshelperstesthelperjs && npm run compile-dataes5identifiernamesjs && npm run compile-dataasciiidentifierdatajs && npm run compile-testshelpersbrowserserverjs && npm run compile-datanonasciiidentifierstartjs && npm run compile-scriptsremovedevdependenciesjs && npm run compile-scriptsgenerateidentifierdatajs && npm run compile-testshelpersbrowserfixturefsjs && npm run compile-datanonasciiidentifierpartonlyjs && npm run compile-srcclijs && npm run compile-srclexjs && npm run compile-srcregjs && npm run compile-srcvarsjs && npm run compile-srcstatejs && npm run compile-srcstylejs && npm run compile-testsclijs && npm run compile-srcjshintjs && npm run compile-srcoptionsjs",
        "compile-2_script": "npm run compile-srcmessagesjs && npm run compile-scriptsbuildjs && npm run compile-testsbrowserjs && npm run compile-srcnamestackjs && npm run compile-srcprodparamsjs && npm run compile-testsunitcorejs && npm run compile-testsunitenvsjs && npm run compile-examplesreporterjs && npm run compile-srcscopemanagerjs && npm run compile-testsunitparserjs && npm run compile-srcreportersunixjs && npm run compile-teststest262testjs && npm run compile-testsunitoptionsjs && npm run compile-srcplatformsrhinojs && npm run compile-teststest262indexjs && npm run compile-teststest262reportjs && npm run compile-srcreportersdefaultjs && npm run compile-testshelpersfixturejs && npm run compile-testsunitmoduleapijs && npm run compile-srcreportersnon_errorjs",
        "compile": "npm run compile-1_script && npm run compile-2_script"
    },
    "main": "./src/jshint.js",
    "dependencies": {
        "cli": "~1.0.0",
        "console-browserify": "1.1.x",
        "exit": "0.1.x",
        "htmlparser2": "3.8.x",
        "lodash": "~4.17.11",
        "minimatch": "~3.0.2",
        "shelljs": "0.3.x",
        "strip-json-comments": "1.0.x"
    },
    "devDependencies": {
        "async": "~2.1.2",
        "browserify": "9.x",
        "conventional-changelog": "0.4.x",
        "conventional-github-releaser": "0.4.x",
        "coveralls": "2.11.x",
        "istanbul": "0.3.x",
        "jscs": "1.11.x",
        "mock-stdin": "0.3.x",
        "nodeunit": "0.9.x",
        "phantom": "~4.0.1",
        "phantomjs-prebuilt": "~2.1.7",
        "regenerate": "1.2.x",
        "results-interpreter": "~1.0.0",
        "sinon": "1.12.x",
        "test262-stream": "~1.2.0",
        "unicode-5.2.0": "^0.7.5",
        "unicode-11.0.0": "0.7.x",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-preset-env": "latest",
        "babel-preset-es2015-without-strict": "latest"
    },
    "license": "(MIT AND JSON)",
    "preferGlobal": true,
    "files": [
        "bin",
        "data",
        "dist",
        "src"
    ]
}